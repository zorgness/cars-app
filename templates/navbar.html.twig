<div class="navbar navbar-expand-sm navbar-dark navbar-lewagon bg-info">
  <div class="container-fluid">
    <a class="navbar-brand" href="{{ path('home')}}">
      <img src="https://www.kernix.com/doc/symfony.svg" />
    </a>
    <h1 class="navbar-brand">Car Park</h1>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>

    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto">
          <li class="nav-item active">
            <a class="nav-link" href="{{ path('home')}}">Home</a>
          </li>
          {% if is_granted('ROLE_USER') or is_granted('ROLE_ADMIN') %}
          <li class="nav-item active">
            <a class="nav-link" href="{{ path('cars')}}">Cars</a>
          </li>
          {% endif %}
          {% if is_granted('ROLE_ADMIN') %}
           <li class="nav-item active">
            <a class="nav-link" href="{{ path('admin')}}">Admin</a>
          </li>
          {% endif %}

          {% if app.user %}
            <li class="nav-item active">
                <a class="nav-link" href="{{ path('logout') }}">Logout</a>
            </li>
            {% else %}
            <li class="nav-item active">
                <a class="nav-link" href="{{ path('register') }}">Register</a>
            </li>

              <li class="nav-item active">
                <a class="nav-link" href="{{ path('login') }}">Login</a>
              </li>
            {% endif %}











          <!--li class="nav-item"-->
          <!--/li-->
      </ul>
    </div>
  </div>
</div>
